{
  "name": "digs-example",
  "version": "0.1.0",
  "description": "Example Digs server",
  "main": "bin/digs.js",
  "bin": {
    "digs-example": "digs.js"
  },
  "dependencies": {
    "digs": "digsjs/digs#coap",
    "digs-serial": "digsjs/digs-serial#coap"
  },
  "devDependencies": {
    "digs-dev": "^0.2.2-6",
    "nodemon": "^1.4.0"
  },
  "scripts": {
    "prepublish": "digs-dev install",
    "start": "nodemon -w node_modules/digs/lib -w node_modules/digs-serial/lib bin/digs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digsjs/digs-example.git"
  },
  "keywords": [
    "digs"
  ],
  "author": "Christopher Hiller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digsjs/digs-example/issues"
  },
  "homepage": "https://github.com/digsjs/digs-example#readme"
}
